    "sass-lint-auto-fix": "~0.21.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "term-color": "^1.0.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "webpack-dev-server": "^3.11.0"
  }
}
